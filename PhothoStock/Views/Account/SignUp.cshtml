@model SignUpData
@{
    Layout = "_Layout";
}
<form asp-action="SignUp" class="form-group" enctype="multipart/form-data">
    <div>
        <label asp-for="Nickname" class="col-form-label"></label>
        <input type="text" asp-for="Nickname" class="form-check-input" />
    </div>
    <div>
        <label asp-for="Email" class="col-form-label"></label>
        <input type="text" asp-for="Email" class="form-check-input" />
    </div>
    <div>
        <label asp-for="Password" class="col-form-label"></label>
        <input asp-for="Password" class="form-check-input" />
    </div>
    <div>
        <label asp-for="ConfirmPassword" class="col-form-label"></label>
        <input asp-for="ConfirmPassword" class="form-check-input" />
    </div>
    <div>
        <div>
            <input type="submit" value="Save" class="btn btn-primary" />
        </div>
    </div>
</form>
@section Scripts
{
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src ="~/lib/jquery-validation-unobtrusive//jquery.validate.unobtrusive.js"></script>
}