@model SignUpData
@{
    Layout = "_Layout";
}
<form asp-action="SignUp"  enctype="multipart/form-data">
    <div class="form-group">
        <div>
            <label asp-for="Nickname" class="col-form-label">Nickname: </label>
        </div>
        <input type="text" asp-for="Nickname" class="form-control" />
    </div>
    <div class="form-group">
        
            <label asp-for="Email" class="col-form-label">Email: </label>
        
        <input type="text" asp-for="Email" class="form-control" />
    </div>
    <div>
        <label asp-for="Password" class="col-form-label">Password: </label>
        <input asp-for="Password" class="form-control" />
    </div>
    <div class="form-group">
         <label asp-for="ConfirmPassword" class="col-form-label">Confirm Password: </label>
        <input asp-for="ConfirmPassword" class="form-control" />
    </div>
        <div>
            <input type="submit" value="Create Account" class="btn btn-primary" />
        </div>
</form>
@section Scripts
{
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive//jquery.validate.unobtrusive.js"></script>
}